<Window x:Class="FromExcelWord.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FromExcelWord"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Menu Background="gray">
            <MenuItem Header="Открыть"/>

        </Menu>
        <Label Content="Введите ссылку на файл Excel"/>
        <TextBox x:Name="textbox" Background="Yellow"/>
        <!--<Button Content="Количество персонала в отделе" Background="Gray" Click="Button_Click"/>
        <Button Content="Количество задач у сотрудника" Background="Gray" Click="Button_Click_1"/>
        <Button Content="Количество задач по отделам" Background="Gray" Click="Button_Click_2"/>
        <Button Content="Количество задач у сотрудников по отделам" Background="Gray" Click="Button_Click_3"/>-->
        <Button Content="Отчет в Word" Background="Gray" Click="Button_Click_4"/>
        <DataGrid x:Name="datagrid1" Height="702"/>
    </StackPanel>
</Window>
